{
  "name": "mashup-template",
  "author": "Antoine Renault <antoine.renault.mmi@gmail.com>",
  "version": "0.1.0",
  "description": "Agora",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "prod": "npm run build && ./node_modules/.bin/serve ./dist --single",
    "dev": "webpack-dev-server --colors",
    "clean": "rimraf dist",
    "watch": "nodemon -e scss -x \"yarn build\"",
    "build": "npm run clean && NODE_ENV=production webpack",
    "deploy:now": "npm run build && cd dist && now --static -p -n agora  && cd ..",
    "deploy:gae:staging": "npm run build && gcloud app deploy --version=staging --no-promote && gcloud app browse --service=staging",
    "deploy:gae:production": "npm run build && gcloud app deploy --version production && gcloud app browse && gcloud app logs tail -s default"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "handlebars": "^3.0.3",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "preload-webpack-plugin": "^1.2.2",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "dotenv-webpack": "^1.5.4",
    "firebase": "^4.3.0",
    "jquery": "^3.2.1",
    "normalize.css": "^7.0.0",
    "serve": "^6.0.6"
  }
}
